    <section>
        <div class="fluid-container" id="header-background">
            <div class="header-content">
            <div class="row">
                <div class="col-12">
                    <h2 class="header-heading ">Pakistan's most trusted platform for Angel Investing 	&nbsp;	&nbsp;	&nbsp;	&nbsp;	&nbsp;	&nbsp;	&nbsp;	&nbsp;	&nbsp;	&nbsp;	&nbsp;	&nbsp;& Startup Fundraising</h2>
                </div>
                <div class="col-sm-6">
                    <div class="invest-flex">
                        <h5>Invest in Pakistan's most promising Startups</h5>
                        <p>Diversify your investment portfolio with access to highly curated and personalised deal flow from India and beyond</p>
                        <div class="invest-btn">
                            <a class="btn btn-primary btn-lg" href="/register" role="button">I am Investor</a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="invest-flex">
                        <h5>Invest in Pakistan's most promising Startups</h5>
                        <p>Diversify your investment portfolio with access to highly curated and personalised deal flow from India and beyond</p>
                        <div class="startup-btn">
                            <a class="btn btn-success btn-lg" href="/register" role="button">I am startup</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </section>
    <!-- Display startups -->
    <section>
    <div class="fuild-container container-gray">
        <div class="row">
            <div class="col">
                <h3 class="startup-heading-display">Find an investment opportunity that's right for you.</h3>
            </div>
        </div>
        <div class="row px-lg-5 startup-section-display">
            <% startups.forEach(startup => { %>
                <div class="col-md-4">
                    <div class="card startup-card" style="height: 500px;">
                        <img class="card-img-top" src="http://via.placeholder.com/640x260" alt="Card image cap">
                        <div class="card-body">
                          <h5 class="card-title"><%= startup.startupName %></h5>
                          <p><span class="material-icons">
                            location_on
                            </span><%= startup.city %>,<%= startup.country %></p>
                          <p class="card-text"><%= startup.startup_summary %></p>
                          <div class="budget">
                            <div class="budget-target">
                                <h5>Rs. <%= startup.total_amount %></h5>
                                <p>Target</p>
                            </div>
                            <div class="budget-min">
                              <h5>Rs. <%= startup.per_investor %></h5>
                              <p>Min Per Investor</p>
                          </div>
                        </div>
                          <a href="/startup-detail-page/<%= startup._id %>" class="btn btn-primary startup-read-more">Read More</a>
                        </div>
                      </div>
                </div>
            <% }) %>
        </div>
    </div>
</section>
    <!-- Section investor -->
    <section>
        <div class="container">
            <div class="row">
                <div class="col">
                    <h3 class="startup-heading-display">We help investors and entrepreneurs build lasting and profitable relationships.</h3>
                </div>
            </div>
            <div class="row pt-5 pb-5 m-auto">
            <% investors.forEach(investor => { %>
            <div class="col-md-6 col-lg-4 pb-3">
        
                <!-- Copy the content below until next comment -->
                <div class="card card-custom bg-white border-white border-0">
                  <div class="card-custom-img" style="background-image: url(http://res.cloudinary.com/d3/image/upload/c_scale,q_auto:good,w_1110/trianglify-v1-cs85g_cc5d2i.jpg);"></div>
                  <div class="card-custom-avatar">
                    <img class="img-fluid" src="data:image/jpeg;base64," alt="Avatar" />
                  </div>
                  <div class="card-body" style="overflow-y: auto">
                    <h5 class="card-title text-center">Rs.<%= investor.Min_Investment %> - Rs.<%= investor.Max_Investment %></h5>
                    <p class="text-center"><span class="material-icons">
                        location_on
                        </span><%= investor.city %>,<%= investor.country %></p>
                    <p class="card-text text-center"><%= investor.professional_bg %></p>
                  </div>
                  <h4 class="text-center">Areas of Expertise</h4>
                  <p class="text-center"><%= investor.Area_Of_Expertise %></p>
                  <div class="card-footer" style="background: inherit; border-color: inherit;">
                    <a href="/investor-detail-page/<%= investor._id%>" class="btn btn-primary invest-read-more">Read More</a>
                  </div>
                  
                </div>
            </div>

                <% }); %>
        </div>
        </div>
        
    </section>

