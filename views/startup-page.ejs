    <!-- Display startups -->
    <section>
        <div class="fuild-container container-gray">
            <div class="row">
                <div class="col">
                    <h3 class="startup-heading-display">Find an investment opportunity that's right for you.</h3>
                </div>
            </div>
            <div class="row px-lg-5 startup-section-display">
                <% startups.forEach(startup => { %>
                    <div class="col-md-4">
                        <div class="card startup-card" style="height: 500px;">
                            <img class="card-img-top" src="http://via.placeholder.com/640x260" alt="Card image cap">
                            <div class="card-body">
                              <h5 class="card-title"><%= startup.startupName %></h5>
                              <p><span class="material-icons">
                                location_on
                                </span><%= startup.city %>,<%= startup.country %></p>
                              <p class="card-text"><%= startup.startup_summary %></p>
                              <div class="budget">
                                <div class="budget-target">
                                    <h5>Rs. <%= startup.total_amount %></h5>
                                    <p>Target</p>
                                </div>
                                <div class="budget-min">
                                  <h5>Rs. <%= startup.per_investor %></h5>
                                  <p>Min Per Investor</p>
                              </div>
                            </div>
                              <a href="/startup-detail-page/<%= startup._id %>" class="btn btn-primary startup-read-more">Read More</a>
                            </div>
                          </div>
                    </div>
                <% }) %>
            </div>
        </div>
    </section>
    
